      }
    },
    {
      "segment_id": "90d35b51",
      "source_content": "### `timestamp`",
      "source_content_hash": "dfc6afafe180dc4bcc0de76c666151e7de939ab4982d7de4771ea90fd4ca87da",
      "node_type": "heading",
      "translatable": true,
      "translations": {
        "zh-hant": "### `timestamp`"
      }
    },
    {
      "segment_id": "86348f98",
      "source_content": "Timestamp value when a PressEvent occurred. Value is represented in milliseconds.",
      "source_content_hash": "346e3a1e59e303cf0ae05d582106570ec4b299e5e97ea95d1db5ed16b10007a7",
      "node_type": "paragraph",
      "translatable": true,
      "translations": {
        "zh-hant": "PressEvent 發生時的時間戳記值。值以毫秒表示。"
      }
    },
    {
      "segment_id": "5ceab032",
      "source_content": "| Type   | Optional |\n| ------ | -------- |\n| number | No       |",
      "source_content_hash": "8b67538dbe61ec5abf75cc712cceb70b45be77ece8c8e812630adff64bc40c0c",
      "node_type": "table",
