    },
    {
      "segment_id": "7f71ff2a",
      "source_content": "`config` is an object containing:",
      "source_content_hash": "431d382469bba9c5d8b8ea9ae4cce90249adea22fd4ce5588922831aa2ac5e9b",
      "node_type": "paragraph",
      "translatable": true,
