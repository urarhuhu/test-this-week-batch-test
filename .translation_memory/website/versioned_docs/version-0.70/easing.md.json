      "source_content_hash": "4f781721bc7f291a0fad801e4007f36e0faa3c28436bdb8c7fd28535b48019bf",
      "node_type": "paragraph",
      "translatable": true,
      "translations": {
        "zh-hant": "向後運行一個緩動函數。"
      }
    },
    {
      "segment_id": "b7adcb7e",
      "source_content": "---",
      "source_content_hash": "cb3f91d54eee30e53e35b2b99905f70f169ed549fd78909d3dac2defc9ed8d3b",
      "node_type": "thematicBreak",
