    {
      "segment_id": "2c1b51df",
      "source_content": "### `disabled`",
      "source_content_hash": "e341f12bd3a5525b08ca6af0655cb7ecea49172a6a89af4f4757af19e4a43874",
      "node_type": "heading",
      "translatable": true,
      "translations": {
        "zh-hant": "### `disabled`"
      }
    },
    {
      "segment_id": "e536ea0d",
      "source_content": "If true the user won't be able to toggle the switch.",
      "source_content_hash": "b09cff19487e2e9ccdae8193448ce930ce3f73b295853d484724b91443be11a7",
      "node_type": "paragraph",
      "translatable": true,
      "translations": {
        "zh-hant": "若為 true，用戶將無法切換開關狀態。"
      }
    },
