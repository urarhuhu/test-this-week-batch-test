    },
    {
      "segment_id": "d7ca1d9e",
      "source_content": "### `in()`",
      "source_content_hash": "3560730e0e63b954311cbb07afe9b7d91fc5e32710509cfab2e19b531e0474b2",
      "node_type": "heading",
      "translatable": true,
      "translations": {
        "zh-hant": "### `in()`"
      }
    },
    {
